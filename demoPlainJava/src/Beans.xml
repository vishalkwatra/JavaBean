<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="idStudent" class="ey.hr.lms.code.Student" init-method="onStart" destroy-method="onEnd">
	<property name="firstName" value="Small"></property>
	<property name="lastName" value="Rabbit"></property>
	<property name="location" value="Tree"></property>
</bean>
                 
</beans>               